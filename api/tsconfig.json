{
    "compilerOptions": {
        "target": "ES2022",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": ".vercel/output",
        "rootDir": ".",
        "declaration": false,
        "resolveJsonModule": true
    },
    "include": [
        "./**/*.ts"
    ]
}